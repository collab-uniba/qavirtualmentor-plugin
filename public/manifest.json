
{
  "manifest_version": 2,
  "name": "QAvMentor",
  "version": "0.0.3",
  "description": "Virtual mentor (Chrome) plugin for social Q&A sites, e.g. Stack Overflow",
  "content_scripts": [
    {
      "matches": ["https://stackoverflow.com/questions/ask"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"],

      "run_at": "document_end"

    }
  ]
}
